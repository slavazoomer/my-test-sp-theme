    "d4ab7a92-9eff-4cdd-83b3-dc766542fa7a": {
      "type": "image-with-text",
      "blocks": {
        "template--15397621530666__d4ab7a92-9eff-4cdd-83b3-dc766542fa7a-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "Buriti Soothing Moisturizer",
